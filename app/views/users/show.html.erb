<h1><%= @user.username %></h1>
<p>@<%= @user.handle %></p>
<p>Joined <%= date_formatted(@user.created_at) %></p>
<%= link_to "#{@user.followers.length} Followers", followers_path(@user.handle) %>
<%= link_to "#{@user.followed_users.length} Following", following_path(@user.handle) %>
<%= render partial: "layouts/posts" %>