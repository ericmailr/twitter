<% if current_user %>
  Logged in as <%= current_user.username %>.
  <%= link_to "Log Out", logout_path %>
<% else %>
  <%= link_to "Log In", login_path %>
<% end %>

<% if current_user %>
    <%= form_for @tweet do |f| %>
        <%= f.text_field :content %><br>
        <%= f.submit 'Tweet' %>
    <% end %>
    <% current_user.tweets.each do |tweet| %>
        <p><%= tweet.content %></p>
    <% end %>
<% end %>
