<% if current_user %>
  Logged in as <%= current_user.username %>.
  <%= link_to "Log Out", logout_path %>
<% else %>
  <%= link_to "Log In", login_path %>
<% end %>

<h1>Tweets#index</h1>

<% if current_user %>
    <% current_user.tweets.each do |tweet| %>
        <p><%= tweet.content %></p>
    <% end %>
<% end %>
